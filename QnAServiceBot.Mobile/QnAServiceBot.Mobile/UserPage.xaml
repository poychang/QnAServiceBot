             x:Class="QnAServiceBot.Mobile.UserPage">
    <ContentPage.Content>
        <StackLayout x:Name="MyStackLayout">
            <Label Text="Welcome to User!"
                   VerticalOptions="CenterAndExpand" 
                   HorizontalOptions="CenterAndExpand" />
