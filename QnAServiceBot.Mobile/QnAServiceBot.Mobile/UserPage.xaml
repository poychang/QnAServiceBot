                   x:Name="EntryToken"
                   HorizontalOptions="FillAndExpand" />
            <!--<WebView x:Name="Browser" VerticalOptions="FillAndExpand" WidthRequest="300" HeightRequest="1000" />-->
        </StackLayout>
    </ContentPage.Content>
</ContentPage>